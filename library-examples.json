  "usage_in_logo_creation": {
    "using_library_assets": {
      "description": "How to use library assets when creating logos",
      "background_example": {
        "endpoint": "POST /api/logo/mobile",
        "request_body": {
          "name": "Logo with Library Background",
          "canvas": {
            "aspectRatio": 1.0,
            "background": {
              "type": "image",
              "image": {
                "type": "library",
                "assetId": "123e4567-e89b-12d3-a456-426614174000",
                "url": "https://example.com/backgrounds/abstract-gradient.jpg"
              }
            }
          },
          "layers": [
            {
              "type": "text",
              "text": {
                "value": "My Logo",
                "fontSize": 48,
                "fontColor": "#ffffff"
              }
            }
          ]
        }
      },
      "icon_example": {
        "endpoint": "POST /api/logo/mobile",
        "request_body": {
          "name": "Logo with Library Icon",
          "layers": [
            {
              "type": "icon",
              "icon": {
                "src": "https://example.com/icons/heart.svg",
                "color": "#ff0000",
                "assetId": "789e0123-e89b-12d3-a456-426614174002"
              }
            },
            {
              "type": "text",
              "text": {
                "value": "Love Brand",
                "fontSize": 36,
                "fontColor": "#000000"
              }
            }
          ]
        }
      }
    }
  },
