{
    "info": {
        "_postman_id": "delete-icons-shapes-2024",
        "name": "Delete Icons and Shapes",
        "description": "Postman collection for deleting icons and shapes from the logo maker API",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [{
            "name": "Delete Icon",
            "event": [{
                "listen": "test",
                "script": {
                    "exec": [
                        "pm.test('Status code is 200 or 404', function () {",
                        "    pm.expect(pm.response.code).to.be.oneOf([200, 404, 400]);",
                        "});",
                        "",
                        "if (pm.response.code === 200) {",
                        "    pm.test('Icon deleted successfully', function () {",
                        "        const jsonData = pm.response.json();",
                        "        pm.expect(jsonData.success).to.be.true;",
                        "        pm.expect(jsonData.message).to.include('deleted');",
                        "    });",
                        "}",
                        "",
                        "if (pm.response.code === 400) {",
                        "    pm.test('Icon is in use', function () {",
                        "        const jsonData = pm.response.json();",
                        "        pm.expect(jsonData.success).to.be.false;",
                        "        pm.expect(jsonData.message).to.include('used');",
                        "    });",
                        "}",
                        "",
                        "if (pm.response.code === 404) {",
                        "    pm.test('Icon not found', function () {",
                        "        const jsonData = pm.response.json();",
                        "        pm.expect(jsonData.success).to.be.false;",
                        "        pm.expect(jsonData.message).to.include('not found');",
                        "    });",
                        "}"
                    ],
                    "type": "text/javascript"
                }
            }],
            "request": {
                "method": "DELETE",
                "header": [],
                "url": {
                    "raw": "{{baseUrl}}/api/logo/icons/:id",
                    "host": [
                        "{{baseUrl}}"
                    ],
                    "path": [
                        "api",
                        "logo",
                        "icons",
                        ":id"
                    ],
                    "variable": [{
                        "key": "id",
                        "value": "{{iconId}}",
                        "description": "Icon UUID to delete"
                    }]
                },
                "description": "Delete an icon from the library. Cannot delete if icon is being used in existing logos. Automatically cleans up icon category assignments."
            },
            "response": []
        },
        {
            "name": "Delete Shape",
            "event": [{
                "listen": "test",
                "script": {
                    "exec": [
                        "pm.test('Status code is 200 or 404', function () {",
                        "    pm.expect(pm.response.code).to.be.oneOf([200, 404]);",
                        "});",
                        "",
                        "if (pm.response.code === 200) {",
                        "    pm.test('Shape deleted successfully', function () {",
                        "        const jsonData = pm.response.json();",
                        "        pm.expect(jsonData.success).to.be.true;",
                        "        pm.expect(jsonData.message).to.include('deleted');",
                        "    });",
                        "}",
                        "",
                        "if (pm.response.code === 404) {",
                        "    pm.test('Shape not found', function () {",
                        "        const jsonData = pm.response.json();",
                        "        pm.expect(jsonData.success).to.be.false;",
                        "        pm.expect(jsonData.message).to.include('not found');",
                        "    });",
                        "}"
                    ],
                    "type": "text/javascript"
                }
            }],
            "request": {
                "method": "DELETE",
                "header": [],
                "url": {
                    "raw": "{{baseUrl}}/api/logo/shapes/:id",
                    "host": [
                        "{{baseUrl}}"
                    ],
                    "path": [
                        "api",
                        "logo",
                        "shapes",
                        ":id"
                    ],
                    "variable": [{
                        "key": "id",
                        "value": "{{shapeId}}",
                        "description": "Shape UUID to delete"
                    }]
                },
                "description": "Delete a shape from the library. Automatically cleans up shape category assignments."
            },
            "response": []
        }
    ],
    "variable": [{
            "key": "baseUrl",
            "value": "http://localhost:3000",
            "type": "string"
        },
        {
            "key": "iconId",
            "value": "",
            "type": "string",
            "description": "UUID of the icon to delete"
        },
        {
            "key": "shapeId",
            "value": "",
            "type": "string",
            "description": "UUID of the shape to delete"
        }
    ]
}